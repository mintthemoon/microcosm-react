{
  "name": "@microcosm/react",
  "version": "0.0.0",
  "description": "React UI components for Cosmos SDK Dapps",
  "author": "mintthemoon <mint@mintthemoon.xyz>",
  "homepage": "https://mintthemoon.xyz",
  "license": "Unlicense",
  "browserslist": ">0.3%, defaults, fully supports es6-module, fully supports bigint, not ie 11",
  "type": "module",
  "scripts": {
    "test": "node __test__/main.test.ts",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "tswc -- lib -d dist --ignore '**/*.d.ts'",
    "clean": "rm -rf dist"
  },
  "exports": {
    ".": { "import": "./dist/index.js", "types": "./dist/types/" },
    "./base.css": "./lib/components/base.css"
  },
  "files": ["lib", "dist"],
  "repository": { "type": "git", "url": "git+https://github.com/mintthemoon/microcosm-react.git" },
  "bugs": { "url": "https://github.com/mintthemoon/microcosm-react/issues" },
  "devDependencies": { "@chain-registry/v2-types": "^0.49.28", "@swc/core": "^1.7.0", "tswc": "^1.2.0" },
  "dependencies": {
    "@chain-registry/v2": "^1.65.28",
    "@cosmjs/amino": "^0.32.4",
    "@cosmjs/cosmwasm-stargate": "^0.32.4",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "chain-registry": "^1.63.33",
    "clsx": "^2.1.1",
    "kujira.js": "github:Team-Kujira/kujira.js",
    "moize": "^6.1.6",
    "tailwind-merge": "^2.4.0"
  }
}
