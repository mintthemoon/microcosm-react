{
  "name": "@microcosm/react",
  "version": "0.0.0",
  "description": "React UI components for Cosmos SDK Dapps",
  "author": "mintthemoon <mint@mintthemoon.xyz>",
  "homepage": "https://mintthemoon.xyz",
  "license": "Unlicense",
  "browserslist": ">0.3%, defaults, fully supports es6-module, fully supports bigint, not ie 11",
  "type": "module",
  "scripts": {
    "test": "node __test__/main.test.ts",
    "dev": "node __bin__/dev-watch.js",
    "build": "npm run build:js && npm run build:css",
    "build:js": "tswc -- lib -d dist --ignore '**/*.d.ts'",
    "build:css": "find ./lib -type f -iname \"*.css\" -exec sh -c 'lightningcss --minify --browserslist -d $(dirname {} | sed s:./lib:./dist:g) {}' \\;",
    "clean": "rm -rf dist",
    "check": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./types/index.d.ts"
    },
    "./utility": {
      "import": "./dist/utility/index.js",
      "types": "./types/utility.d.ts"
    },
    "./base.css": "./lib/components/base.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mintthemoon/microcosm-react.git"
  },
  "bugs": {
    "url": "https://github.com/mintthemoon/microcosm-react/issues"
  },
  "devDependencies": {
    "@chain-registry/v2-types": "^0.49.28",
    "@keplr-wallet/types": "^0.12.111",
    "@swc/core": "^1.7.0",
    "@types/lodash": "^4.17.7",
    "globals": "^15.8.0",
    "lightningcss-cli": "^1.25.1",
    "node-jq": "^6.0.1",
    "tswc": "^1.2.0"
  },
  "dependencies": {
    "@chain-registry/v2": "^1.65.28",
    "@cosmjs/amino": "^0.32.4",
    "@cosmjs/cosmwasm-stargate": "^0.32.4",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "@parcel/watcher": "^2.4.1",
    "@uidotdev/usehooks": "^2.4.1",
    "clsx": "^2.1.1",
    "kujira.js": "github:mintthemoon/kujira.js#dev",
    "lodash": "^4.17.21",
    "moize": "^6.1.6",
    "tailwind-merge": "^2.4.0"
  }
}
